<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.my_hostel">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SET_ALARM"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        >
        <activity android:name=".SplashActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".Authentication"
            android:label="Student Login" >
        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".MainActivity"
            android:label="Welcome Student" >

        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".Admin"
            android:label="Admin Login" >
        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".AccountType"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".AdminView"
            android:label="Welcome Admin" >
        </activity>
        <receiver android:name=".Notification_receiver"/>

        <activity
            android:theme="@style/AppTheme1"
            android:name=".Notice"
            android:label="Broadcast Notice" >
        </activity>
        <activity
            android:theme="@style/AppTheme1"
            android:name=".NoticeStudent"
            android:label="Notices" >
        </activity>





    </application>

</manifest>